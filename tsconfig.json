{
  "compilerOptions": {
    // "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "types": ["vite/client","node"],
    "baseUrl": "./",
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": ["src/render/*"],
      "root/*": ["/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*",
    "typings/*.d.ts"
  ]
}
